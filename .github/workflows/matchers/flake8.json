{
    "problemMatcher": [
        {
            "owner": "flake8_severity",
            "pattern": [
              {
                "regexp": "^##\\[([a-z]+)\\](\\S+):(\\d+):(\\d+): ([A-Z]\\d+) (.*)",
                "severity": 1,
                "file": 2,
                "line": 3,
                "column": 4,
                "code": 5,
                "message": 6
              }
            ]
          },
        {
            "owner": "flake8",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "(\\S+):(\\d+):(\\d+): ([A-Z]\\d+) (.*)",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}