{
    "problemMatcher": [
      {
        "owner": "isort",
        "severity": "error",
        "pattern": [
          {
            "regexp": "^(\\S*): (\\S*) (.*)$",
            "file": 2,
            "severity": 1,
            "message": 3
          }
        ]
      },
      {
        "owner": "isort_severity",
        "pattern": [
          {
            "regexp": "^#{0,2}\\[([a-z]*)\\](\\S*): (\\S*) (.*)$",
            "file": 3,
            "severity": 1,
            "message": 4
          }
        ]
      }
    ]
  }
  