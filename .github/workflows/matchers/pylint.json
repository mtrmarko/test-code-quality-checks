{
  "problemMatcher": [
    {
      "owner": "pylint-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(\\S+):(\\d+):(\\d+): ([A-Z]\\d+):(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5,
          "loop": true
        }
      ]
    }
  ]
}
