{
  "problemMatcher": [
    {
      "owner": "black",
      "severity": "error",
      "column": 1,
      "line": 1,
      "code": "W",
      "pattern": [
        {
          "regexp": "^(would reformat) (.*)$",
          "file": 2,
          "message": 1
        }
      ]
    },
    {
      "owner": "black_severity",
      "column": 1,
      "line": 1,
      "code": "W",
      "pattern": [
        {
          "regexp": "^#{0,2}\\[([a-z]*)\\](would reformat) (.*)$",
          "severity": 1,
          "file": 3,
          "message": 2
        }
      ]
    }
  ]
}
